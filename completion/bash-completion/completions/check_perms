# mailman check_perms completion                           -*- shell-script -*-

_check_perms()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '-f -v -h' -- "$cur" ) )
    fi

} &&
complete -F _check_perms check_perms

# ex: ts=4 sw=4 et filetype=sh
