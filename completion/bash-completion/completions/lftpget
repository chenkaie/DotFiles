# lftpget(1) completion                                    -*- shell-script -*-

_lftpget()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]] ; then
        COMPREPLY=( $( compgen -W '-c -d -v' -- "$cur" ) )
    fi
} &&
complete -F _lftpget lftpget

# ex: ts=4 sw=4 et filetype=sh
