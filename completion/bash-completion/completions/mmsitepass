# mailman mmsitepass completion                            -*- shell-script -*-

_mmsitepass()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '--listcreator --help' -- "$cur" ) )
    fi

} &&
complete -F _mmsitepass mmsitepass

# ex: ts=4 sw=4 et filetype=sh
