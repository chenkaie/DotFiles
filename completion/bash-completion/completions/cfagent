# cfagent completion                                       -*- shell-script -*-

_cfagent()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -f|--file)
            _filedir
            return 0
            ;;
    esac

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '$( _parse_help "$1" )' -- "$cur" ) )
    fi
} &&
complete -F _cfagent cfagent

# ex: ts=4 sw=4 et filetype=sh
